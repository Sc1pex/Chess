\lstdefinelanguage{RustHtml}{
	morecomment=[l]{//},
	morestring=[b]{"},
	alsodigit={},
	alsoletter={!},
	keywords=[4]{pub, async, fn, let, if, else, return, mut, const, for},
	keywords=[2]{self, this},
	keywords=[3]{Option, String, str, Vec, struct, bool, u8, u32, User, Game, Board, BitBoard, Color, Square, State, TypedHeader, Json, GameDataJson, AppState, Cookie, GameState, Some, None, i64, int},
	keywords={fetch, match, div, html, body, table, tr, td, title, form, button, a, th, p, select, option, input, span, label, imgd, h1, h2},
	otherkeywords={<, >},
}

\lstdefinestyle{boxed}{
	numbers=left,
	firstnumber=auto,
	numberblanklines=true,
	frame=trbL,
	numberstyle=\tiny,
	frame=leftline,
	numbersep=7pt,
	framesep=5pt,
	framerule=15pt,
	xleftmargin=15pt,
	backgroundcolor={\color{ctpBase}},
	rulecolor={\color{ctpMantle}},
	stringstyle = {\color{ctpGreen}},
	commentstyle={\color{ctpOverlay1}},
	basicstyle = {\linespread{1.05}\small\color{ctpText}\ttfamily},
	keywordstyle = [4]{\color{ctpMauve}},
	keywordstyle = [2]{\color{ctpBlue}},
	keywordstyle = [3]{\color{ctpYellow}},
	keywordstyle = {\color{ctpLavender}},
	keywordstyle = [5]{\color{ctpPeach}},
	columns=spaceflexible,
	keepspaces=true,
	showspaces=false,
	showtabs=false,
	showstringspaces=false,
	breaklines=true,
}
